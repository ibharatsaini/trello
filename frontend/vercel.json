{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors http://localhost http://169.254.169.254 https://google.com http://127.0.0.1"
        }
      ]
    
    }
]

}